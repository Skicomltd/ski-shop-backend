{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "ts-node db/generate-migration.ts",
    "migration:drop": "typeorm-ts-node-commonjs schema:drop -d ./db/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./db/typeorm.config.ts",
    "migration:migrate": "typeorm-ts-node-commonjs migration:run -d ./db/typeorm.config.ts",
    "seed": "nest build && node dist/src/seeder",
    "seed:refresh": "node dist/src/seeder --refresh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
