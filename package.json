    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/node": "^9.34.0",
    "@sentry/profiling-node": "^9.34.0",
    "archiver": "^7.0.1",
